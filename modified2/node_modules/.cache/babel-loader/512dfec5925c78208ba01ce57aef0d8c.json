>\n    <div style=\"color: #f00\" v-show=\"isConnected\">Connect Success!</div>\n    <button v-show=\"isConnected\" @click=\"approveMoney\">approve $100</button>\n  </div>\n\n  <div class=\"row\">\n    <div class=\"col-lg-12\">\n        <h1 class=\"text-center\">LOT TOKEN ICO SALE</h1>\n        <div>\n          <button @click=\"buyTokens\" >Connect to Wallet</button>\n        </div>\n        <hr/>\n        <br/>\n    </div>\n    <div id=\"content\" class=\"text-center\">\n      <p>\n        Introducing \"Lot Token\"!\n        Token price is <span class=\"token-price\"></span> Ether. You currently have <span class=\"lot-balance\"></span>&nbsp;Lot.\n      </p>\n      <br/>\n      <form onSubmit=\"buyTokens\" role=\"form\">\n      <!-- <form role = \"form\"> -->\n        <div class=\"form-group\">\n          <div class=\"input-group\">\n            <input id=\"numberOfTokens\" class=\"form-control input-lg\" type=\"number\" name=\"number\" value=\"1\" min=\"1\" pattern=\"[0-9]\">\n            <span class=\"input-group-btn\">\n              <button type=\"submit\" class=\"btn btn-primary btn-lg\" >Buy Tokens</button>\n            </span>\n          </div>\n        </div>\n      </form>\n      <br>\n       <div class=\"progress\">\n          <div id=\"progress\" class=\"progress-bar progress-bar-striped active\" aria-valuemin=\"0\" aria-valuemax=\"100\">\n          </div>\n        </div>\n      <p><span class=\"tokens-sold\"></span> / <span class=\"tokens-available\"></span> tokens sold</p>\n      <hr>\n      <p id=\"accountAddress\"></p>\n    </div>\n  </div>\n</div>\n\n\n\n</template>\n\n<script>\nimport web3 from 'web3'\nimport services from \"@/api\";\nimport $ from 'jquery'\nexport default {\n  data() {\n    return {\n      account: \"\",\n      isConnected: false,\n      key:0,\n      remain_token:0,\n      tokensAvailable: 750000,\n      tokenPrice: \"1000000000000000\",\n    };\n  },\n  watch: {\n    \"$store.state.dapp\": {\n      handler(val) {\n        this.account = val.account;\n        this.isConnected = val.isConnected;\n      },\n      immediate: true,\n      deep: true,\n    },\n  },\n  methods: {\n    connect() {\n      window[\"aleereum\"] && window[\"aleereum\"].connect();\n      this.key++;\n      this.isConnected=true;\n    },\n    approveMoney() {\n      services.getSymbol().then(res => {\n\n        console.log(res);\n      });\n      //services.approve(100).then((res) => {\n      //  console.log(res);\n      //});\n    },\n    buyTokens() {\n\n      services.buy_token(100).then((res) => {\n        console.log(res);\n      });\n\n    },\n\n  },\n  async created(){\n      console.log(services.getName());\n      services.getName().then(res => {\n        console.log(\"res\");\n        console.log(res);\n      });\n      let curr_res=await services.getRemain();\n      console.log(\"curr_res\")\n      console.log(curr_res)\n      services.getRemain().then(res => {\n        this.remain_token=res;\n        console.log(\"res\")\n        console.log(res)\n        $('.token-price').html(web3.utils.fromWei(this.tokenPrice, \"ether\"));\n        $('.tokens-sold').html(this.tokensAvailable-this.remain_token);\n        $('.tokens-available').html(this.remain_token);\n        var progressPercent = (Math.ceil(this.remain_token) / this.tokensAvailable) * 100;\n        $('#progress').css('width', progressPercent + '%');\n      });\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]}]}